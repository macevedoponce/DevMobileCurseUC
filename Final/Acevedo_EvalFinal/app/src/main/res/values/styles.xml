<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Base.Widget.MaterialComponents.TextInputLayout.OutlinedBox"/>

    <style name="Acevedo_EvalFinal_Principal" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/secondary</item>
        <item name="colorAccent">@color/secondary_secondary</item>

        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <item name="android:navigationBarColor" tools:targetApi="l">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>

    </style>

    <style name="Acevedo_EvalFinal_Principal_night" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/secondary_secondary</item>
        <item name="colorPrimaryDark">@color/primary</item>
        <item name="colorAccent">@color/secondary_secondary</item>

    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>

</resources>